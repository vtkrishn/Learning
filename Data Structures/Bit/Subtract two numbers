public class YourClassNameHere {
	    public static void main(String[] args) {
        subtract(5,2);
        subtract(15,7);
	    }
	    
	    public static void subtract(int a, int b){
	        while(b != 0){
	         int borrow = ~a & b;
	         a = a^ b;
	         b = borrow << 1;
	        }
	       System.out.println(a);
	    }
	    
}
	    
      
